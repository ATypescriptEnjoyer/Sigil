{
  "name": "sigil",
  "productName": "Sigil",
  "leagueVersion": "10.21.1",
  "version": "1.0.2",
  "description": "My Electron application description",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make --arch x64",
    "test": "node --version",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts ."
  },
  "keywords": [],
  "author": {
    "name": "Sasha",
    "email": "8694395+SashaRyder@users.noreply.github.com"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {
        "icon": "src/images/icon"
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "sigil"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/sidebar/index.html",
                  "js": "./src/sidebar/renderer.ts",
                  "name": "main_window"
                },
                {
                  "html": "./src/header/header.html",
                  "js": "./src/header/header.ts",
                  "name": "main_header"
                }
              ]
            }
          }
        ]
      ]
    }
  },
  "devDependencies": {
    "@electron-forge/cli": "6.0.0-beta.53",
    "@electron-forge/maker-deb": "6.0.0-beta.53",
    "@electron-forge/maker-rpm": "6.0.0-beta.53",
    "@electron-forge/maker-squirrel": "6.0.0-beta.53",
    "@electron-forge/maker-zip": "6.0.0-beta.53",
    "@electron-forge/plugin-webpack": "6.0.0-beta.53",
    "@marshallofsound/webpack-asset-relocator-loader": "^0.5.0",
    "@types/lodash": "^4.14.162",
    "@types/node": "^12.12.67",
    "@types/node-powershell": "^3.1.1",
    "@types/ps-node": "^0.1.0",
    "@types/ws": "^7.2.7",
    "@typescript-eslint/eslint-plugin": "^3.9.0",
    "@typescript-eslint/parser": "^3.9.0",
    "css-loader": "^4.2.1",
    "electron": "10.1.3",
    "eslint": "^7.6.0",
    "eslint-plugin-import": "^2.20.0",
    "fork-ts-checker-webpack-plugin": "^5.0.14",
    "node-loader": "^1.0.1",
    "style-loader": "^1.2.1",
    "ts-loader": "^8.0.2",
    "tslint": "^6.1.2",
    "typescript": "^4.0.2",
    "file-loader": "^6.1.1"
  },
  "dependencies": {
    "custom-electron-titlebar": "^3.2.5",
    "electron-fetch": "^1.7.1",
    "electron-squirrel-startup": "^1.0.0",
    "electron-store": "^6.0.1",
    "lodash": "^4.17.20",
    "node-powershell": "^4.0.0",
    "sub-events": "^1.8.6",
    "ws": "^7.3.1"
  }
}
